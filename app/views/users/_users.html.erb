<% if users.any? %>
    <ul>
        <% users.each do |user| %>
        <li>
            <div>
                <%= user.name %>
            </div>
            <div>
                <p><%= link_to 'プロフィール', user_path(user), class: "text-decoration-none" %></p>
            </div>
        </li>
        <% end %>
    </ul>
<% end %>